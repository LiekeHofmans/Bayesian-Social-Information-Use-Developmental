        <Param name="Before Experiment" updates="constant" val="import pandas as pd&amp;#10;import numpy as np&amp;#10;from numpy import random&amp;#10;from random import sample&amp;#10;from random import shuffle &amp;#10;import math" valType="extendedCode"/>
